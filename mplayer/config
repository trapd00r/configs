#}}}


[hd]
lavdopts=fast=1:skiploopfilter=all
vfm=ffmpeg

[hd-low]
lavdopts=fast=1:skiploopfilter=all:lowres=1
vfm=ffmpeg

[reset]
brightness=0
contrast=0
hue=0
saturation=0

[gapless]
ao=pcm:nowaveheader:fast:file=aufifo
vc=null
vo=null

